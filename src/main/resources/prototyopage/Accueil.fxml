<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane fx:id="rootPane" prefHeight="358.0" prefWidth="726.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="prototyopage.AccueilController">
   <children>
      <VBox alignment="CENTER" prefHeight="358.0" prefWidth="726.0" spacing="20.0">
          <padding>
              <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
          </padding>
      
          <Label fx:id="welcomeText" />
          <Button onAction="#onHelloButtonClick" text="Hello!" />
         <Button mnemonicParsing="false" onMouseClicked="#showConnection" text="Connexion" />
         <Button mnemonicParsing="false" onMouseClicked="#showSearchBar" text="Recherche" />
      </VBox>
   </children>
</AnchorPane>
